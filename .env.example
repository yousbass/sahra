# Firebase Configuration
# Get these values from: Firebase Console > Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Stripe Configuration
# Get your publishable key from: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here

# Backend API Configuration
# Set this to your backend API URL in production
VITE_API_URL=http://localhost:3001

# Email Service Configuration
# Get your API key from https://resend.com/api-keys
VITE_RESEND_API_KEY=re_your_api_key_here

# Support Contact Information
# Update these with your actual support contact details
VITE_SUPPORT_EMAIL=support@sahra.com
VITE_SUPPORT_PHONE=+973 XXXX XXXX